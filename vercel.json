{
  "version": 2,
  "builds": [
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/web/landing.html"
    },
    {
      "src": "/dashboard",
      "dest": "/web/dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
